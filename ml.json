{
    "method": "MostOccurentColor",
    "method_options": {
        "flush_idle": 15,
        "flush_interval": 5
    },
    "datasets": [
        {
            "class": "krusty",
            "path": "dataset/krusty_the_clown",
            "executor_kwargs": {
                "max_workers": 100
            },
            "pattern": {
                "max_results": 50,
                "output": "krusty.json"
            }
        },
        {
            "class": "otto",
            "path": "dataset/otto_mann",
            "executor_kwargs": {
                "max_workers": 1
            },
            "pattern": {
                "max_results": 50,
                "output": "otto.json"
            }
        }
    ]
}